<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wfzcx.credit">

    <application
        android:name=".app.APP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <meta-data
            android:name="MODEL"
            android:value="
            com.wfzcx.credit.data.model.AccountModel
            " />

        <activity
            android:name=".LaunchActivity"
            android:theme="@style/LaunchTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />

        <activity android:name=".module.main.CreditSearchActivity" />

        <activity android:name=".module.main.ents.CreditEntsListActivity" />

        <activity android:name=".module.main.ents.CreditEntInfoActivity" />

        <activity android:name=".module.login.LoginActivity" />

    </application>

</manifest>